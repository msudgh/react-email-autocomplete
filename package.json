{
  "name": "react-email-autocomplete",
  "version": "1.3.4",
  "description": "Autocomplete React component for email fields",
  "repository": {
    "type": "git",
    "url": "https://github.com/msudgh/react-email-autocomplete.git"
  },
  "author": "Masoud Ghorbnai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msudgh/react-email-autocomplete/issues"
  },
  "main": "index.js",
  "homepage": "https://github.com/msudgh/react-email-autocomplete",
  "keywords": [
    "react-component",
    "react",
    "autocomplete",
    "email",
    "input"
  ],
  "watch": {
    "prepublish": "src/*.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "watch": "npm-watch",
    "deploy": "gh-pages -d build",
    "build": "rollup -c -o index.js",
    "test": "jest --env=jsdom",
    "formatter": "prettier --write --config .prettierrc '**/*.{md,js,yaml,yml}'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.+(md|js|yaml|yml)": [
      "npm run formatter"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.16.7",
    "@babel/core": "7.16.7",
    "@babel/preset-env": "7.16.7",
    "@babel/preset-react": "7.16.7",
    "@babel/runtime": "7.16.7",
    "@types/jest": "27.4.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "27.4.5",
    "babel-loader": "8.2.3",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "gh-pages": "3.2.3",
    "husky": "^7.0.4",
    "jest": "27.4.5",
    "jsdom": "19.0.0",
    "lint-staged": "^12.1.4",
    "nodemon": "2.0.15",
    "npm-watch": "0.11.0",
    "prettier": "2.5.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-test-renderer": "^17.0.2",
    "rollup": "2.62.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
